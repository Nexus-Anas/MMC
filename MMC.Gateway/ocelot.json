{
  "GlobalConfiguration": {
    "BaseUrl": "https://localhost:44327"
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/gateway/Event/FindAll",
      "UpstreamHttpMethod": [ "Get" ],
      "DownstreamPathTemplate": "/api/Event/FindAll",
      "DownstreamScheme": "https",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 44396
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "10s",
        "PeriodTimespan": 10,
        "Limit": 15
      },
      "FileCacheOptions": {
        "TtlSeconds": 10
      },
      "HealthChecks": [
        {
          "Key": "FindAllEvents",
          "Timeout": 5000,
          "Uri": "/health"
        }
      ]
    },
    {
